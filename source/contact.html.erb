<div class="glass-card">
  <% form_tag 'https://formsubmit.co/32403e36674cfd401ce9b45c4bf85aa6', class: "form-grid", method: 'post' do %>
    <div class="form-group-settings">
      <%= label_tag :name %>
      <%= text_field_tag :name %>
    </div>
    <div class="form-group-settings">
      <%= label_tag :email%>
      <%= email_field_tag :email %>
    </div>
    <div class="form-group-settings">
      <%= label_tag :subject %>
      <%= text_field_tag :subject %>
    </div>
    <div class="form-group-settings full-width">
      <%= label_tag :message %>
      <%= text_area_tag :message %>
    </div>
    <div class="btn-group">
      <%= submit_tag "Submit", class: "btn" %>
    </div>
  <% end %>
</div>
